#include <stdio.h>

int main(){

   int L;
   char T[2];
   double M[12][12];
   double soma = 0.0;

   scanf("%d", &L); 

   scanf("%s", &T);

   for(int i = 0; i < 12; i++){
       for(int j = 0; j < 12; i++){
           scanf("%lf", & M[i][j]); 
       }
    }
      
    for(int i = 0; i < 12; i++){
        soma += M[L][i];
    }
    
    if(T[0] == 'S'){
        printf("%.1lf\n", soma);
    }
    else if(T[0] == 'M'){
        printf("%.1lf\n", soma/12.0);
    } 

   return 0;
}
