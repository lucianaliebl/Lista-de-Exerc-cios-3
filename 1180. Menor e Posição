#include <stdio.h>

int main(){

   int N;

   scanf("%d", &N); 

   int X[N];
   int menor;
   int posicao;

   scanf("%d", &X[0]);
   menor = X[0];
   posicao = 0; 

   for(int i = 1; i < N; i++){
       scanf("%d", &X[i]);
       if(X[i] < menor){
          menor = X[i];
          posicao = i;
       }
   }
   
   printf("Menor valor: %d\n", menor);
   printf("Posicao: %d", posicao);


    return 0;
}
