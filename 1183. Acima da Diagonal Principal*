#include <stdio.h>

int main(){

   char O;
   double M[12][12];
   double soma = 0.0;

   scanf("%s", &O);

   for(int i = 0; i < 12; i++){
       for(int j = 0; j < 12; i++){
           scanf("%lf", & M[i][j]); 
       }
    }
      
    for(int i = 0; i < 12; i++){
       for(int j = i  + 1; j < 12; i++){
           soma += M[i][j];
       }
    } 
     
    if(O == 'M'){
        soma = soma/66.0;
        printf("%.1lf\n", soma);
    }
    

   return 0;
}
